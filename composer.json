{
    "name": "alanseiden/doctrine-dbal-ibmi",
    "description": "Doctrine DBAL drivers for DB2 on the IBM i platform",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        },
        {
            "name": "Cassiano Vailati",
            "email": "c.vailati@esconsulting.it"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "doctrine/dbal": "^2.13.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "phpstan/phpstan": "^1.4.10",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpunit/phpunit": "^6.5 || ^9.6",
        "rector/rector": "^0.12.19"
    },
    "suggest": {
        "ext-ibm_db2": "Extension to use the DB2Driver driver",
        "ext-pdo_odbc": "This is the recommended extension to connect with IBM DB2 for i"
    },
    "autoload": {
        "psr-4": {
            "DoctrineDbalIbmi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineDbalIbmi\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
